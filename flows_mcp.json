    {
        "id": "785750e5.ec05f",
        "type": "function",
        "z": "9bb78743.299da8",
        "name": "getClientIdForMaster",
        "func": "var masterName = global.get(\"dreherMaster\")\n\nvar clientsInGroup = \n    msg.payload.result.server.groups\n    .filter(group => findClientsInGroupWithMasterName(group, masterName))[0]\n    .clients.filter(client => client.connected)\n    .map(c => c.id)\n\n msg.payload = clientsInGroup;\n \n return msg;\n \n function findClientsInGroupWithMasterName(group, masterName){\n     return group.clients\n       .filter(client => client.host.name === masterName).length>0\n     \n }",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "x": 740,
        "y": 120,
        "wires": [
